             xmlns:local="clr-namespace:EditorResources.Windows"
             xmlns:UserControls="clr-namespace:EditorResources.UserControls" x:Class="EditorResources.Windows.VariableListSelectorWindow"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>

        <UserControls:VariableListView x:Name="ListView" HorizontalAlignment="Left" VerticalAlignment="Top" Height="377" Width="792" Filter="Unknown">
